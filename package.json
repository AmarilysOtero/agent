{
  "name": "rag-file-scanner",
  "version": "1.0.0",
  "description": "Modern web application for scanning files from multiple sources",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --signal SIGTERM --delay 1000ms server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": ["rag", "file-scanner", "react", "node.js"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "googleapis": "^126.0.1",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "@microsoft/microsoft-graph-client": "^3.0.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
